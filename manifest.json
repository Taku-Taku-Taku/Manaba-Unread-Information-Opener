{
    "manifest_version": 3,
    "name": "Manaba Unread Opener",
    "version": "1.0",
    "description": "Manabaの未読お知らせを別タブで自動的に開く拡張機能",
    "permissions": [
        "scripting",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "未読お知らせを開く"
    }
}